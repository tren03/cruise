{
  "manifest_version": 3,
  "name": "cruise",
  "version": "1.0",
  "description": "Fuzzy-find open tabs in Chrome",
  "action": {
    "default_popup": "./dist/popup.html",
    "default_title": "Fuzzy Tab Finder"
  },
  "permissions": ["tabs","history"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Y"
      },
      "description": "Open cruise dialog"
    }
  }
}

